{
  "name": "tascurator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint . --fix",
    "format": "prettier . --write",
    "format-file-patterns": "prettier \"./src/**/*.{js,jsx,ts,tsx,json,css}\" --write",
    "format-ignore-path": "prettier . --write --ignore-path {any file}",
    "prepare": "husky",
    "typedoc": "typedoc",
    "prisma": "dotenv -e .env.local -- prisma studio",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:push": "dotenv -e .env.local -- prisma db push",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:reset": "dotenv -e .env.local -- prisma migrate reset",
    "prisma:seed": "dotenv -e .env.local -- prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.3.0",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.15.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "dotenv-cli": "^7.4.2",
    "hono": "^4.4.6",
    "lucide-react": "^0.352.0",
    "next": "14.1.2",
    "next-auth": "^5.0.0-beta.19",
    "react": "^18",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18",
    "react-hook-form": "^7.51.0",
    "react-markdown": "^9.0.1",
    "recharts": "^2.12.2",
    "resend": "^3.3.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.2",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "3.2.5",
    "prisma": "^5.15.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.13",
    "typedoc-theme-hierarchy": "^4.1.2",
    "typescript": "^5"
  }
}
